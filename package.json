{
  "name": "diagnosys",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "concurrently": "^9.2.1",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "dev:db": "docker compose up -d db",
    "dev:backend": "npm --workspace apps/backend run start:dev",
    "dev:frontend": "npm --workspace apps/frontend run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  }
}
